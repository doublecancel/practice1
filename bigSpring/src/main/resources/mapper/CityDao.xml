<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="scan.dao.CityDao">

	<select id="findCityById" parameterType="java.lang.Long" resultType="scan.entity.City">
		SELECT * FROM world.city a where a.id = #{id}
	</select>

</mapper>